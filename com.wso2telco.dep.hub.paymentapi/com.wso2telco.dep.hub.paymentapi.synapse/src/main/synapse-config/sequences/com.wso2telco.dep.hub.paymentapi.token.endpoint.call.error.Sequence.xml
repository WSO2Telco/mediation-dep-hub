<?xml version="1.0" encoding="UTF-8"?>
<sequence
  name="com.wso2telco.dep.hub.paymentapi.token.endpoint.call.error.Sequence"
  trace="disable" xmlns="http://ws.apache.org/ns/synapse">
  <filter regex="false"
    source="boolean(get-property('axis2', 'HTTP_SC'))" xmlns:ns="http://org.apache.synapse/xsd">
    <then>
      <log level="custom">
        <property name="tokenEndpointCallErrorSeq" value="Token Pool request timed out. Using default package type."/>
      </log>
    </then>
    <else/>
  </filter>
  <sequence key="com.wso2telco.dep.hub.paymentapi.default.package.type.set.Sequence"/>
</sequence>
